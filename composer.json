{
    "name": "frengky/fineract-client-php",
    "description": "Apache Fineract client library for PHP to interact with the Apache Fineract 1.x Platform APIs",
    "keywords": [
        "apache",
        "fineract",
        "sdk",
        "php",
        "api"
    ],
    "homepage": "https://github.com/frengky/fineract-client-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Frengky",
            "email": "frengky.lim@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.5.5|^7.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "Frengky\\Fineract\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Frengky\\Fineract\\" : "test/" }
    }
}
